<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schoolsystemproject.FXMLDocumentController">
   <children>
      <Label alignment="TOP_LEFT" layoutX="106.0" layoutY="24.0" text="School System">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox layoutX="10.0" layoutY="58.0" prefHeight="140.0" prefWidth="300.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Courses" textAlignment="CENTER">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <Button fx:id="courses" mnemonicParsing="false" onAction="#switchToCourses" text="View Courses" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label alignment="TOP_CENTER" text="Professors">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <Button fx:id="professors" mnemonicParsing="false" onAction="#switchToProfessors" text="View Profs" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label alignment="TOP_LEFT" text="Students">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <Button fx:id="students" mnemonicParsing="false" onAction="#switchToStudents" text="View Students" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
