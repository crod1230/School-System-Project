<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schoolsystemproject.ProfessorAddController">
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Professor List">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Label>
                  <ListView fx:id="professorList" prefHeight="250.0" prefWidth="174.0">
                     <VBox.margin>
                        <Insets left="20.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </ListView>
                  <Button fx:id="removeProfessor" mnemonicParsing="false" onAction="#removeProfessor" text="Remove">
                     <VBox.margin>
                        <Insets left="10.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                  <Label fx:id="selectProfessor" text="**Please Select a Professor**" textFill="#db2727">
                     <font>
                        <Font name="System Italic" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Add Professor">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Label>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label text="Name: ">
                           <HBox.margin>
                              <Insets right="7.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="name" prefHeight="25.0" prefWidth="145.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </TextField>
                     </children>
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label text="DoB: ">
                           <HBox.margin>
                              <Insets right="14.0" />
                           </HBox.margin>
                        </Label>
                        <DatePicker fx:id="dob" prefHeight="25.0" prefWidth="145.0">
                           <HBox.margin>
                              <Insets left="2.0" />
                           </HBox.margin>
                        </DatePicker>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label prefWidth="44.0" text="EduLvl: " />
                        <TextField fx:id="edulvl" prefHeight="25.0" prefWidth="145.0">
                           <padding>
                              <Insets right="3.0" />
                           </padding>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label text="Dpt:">
                           <HBox.margin>
                              <Insets right="22.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="dpt" prefHeight="25.0" prefWidth="145.0" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="45.0" prefWidth="200.0">
                     <children>
                        <Label text="ID: ">
                           <HBox.margin>
                              <Insets right="27.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="finalID" prefHeight="25.0" prefWidth="145.0" />
                     </children>
                  </HBox>
                  <Button fx:id="addProfessor" mnemonicParsing="false" onAction="#addProfessor" text="Add">
                     <VBox.margin>
                        <Insets bottom="10.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                  <Label fx:id="enterFields" text="**Please Enter All Fields**" textFill="#db2f2f">
                     <font>
                        <Font name="System Italic" size="12.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Courses Available">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin></Label>
                  <ListView fx:id="coursesAvailable" prefHeight="250.0" prefWidth="170.0">
                     <VBox.margin>
                        <Insets left="10.0" right="20.0" top="20.0" />
                     </VBox.margin>
                  </ListView>
                  <VBox alignment="CENTER" prefHeight="92.0" prefWidth="200.0">
                     <children>
                        <HBox alignment="CENTER" prefHeight="13.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="addCourse" mnemonicParsing="false" onAction="#addCourseToProfessor" text="Add">
                                 <HBox.margin>
                                    <Insets right="50.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font size="13.0" />
                                 </font>
                              </Button>
                              <Button fx:id="back" mnemonicParsing="false" onAction="#returnToMain" text="Return">
                                 <font>
                                    <Font size="13.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </HBox>
                        <Label fx:id="selectCourse" text="**Please Select a Course**" textFill="#db1e1e">
                           <font>
                              <Font name="System Italic" size="12.0" />
                           </font>
                           <VBox.margin>
                              <Insets right="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets top="11.0" />
                     </VBox.margin>
                  </VBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
